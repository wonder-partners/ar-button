{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": "off",
        "no-undef": "off",
        "no-console": "off",
        "import/extensions": "off",
        "import/prefer-default-export": "off"
    },
    "plugins": [
        "eslint-plugin-html"
    ]
}