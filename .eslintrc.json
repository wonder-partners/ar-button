{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "nonblock-statement-body-position": "off",
        "curly": "off",
        "no-param-reassign": "off",
        "object-curly-newline": "off",
        "linebreak-style": "off",
        "no-undef": "off",
        "no-console": "off",
        "import/extensions": "off",
        "import/prefer-default-export": "off"
    },
    "plugins": [
        "eslint-plugin-html"
    ]
}